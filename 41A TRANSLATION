import java.util.*;
public class Main
{
    public static void main(String args[])
    {
    Scanner sc=new Scanner(System.in);
    String s=sc.next();
    String z=sc.next();
    String p="";
    for(int i=s.length()-1;i>=0;i--)
    {
        p=p+s.charAt(i);
    }
    if(p.equals(z))
    System.out.println("YES");
    else
    System.out.println("NO");
    }
}
