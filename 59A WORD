#include<iostream>
using namespace std;
int main()
{
 string s;
 cin>>s;
 int u,l=0;
 for(int i=0;i<s.size();i++)
 {
     if(isupper(s[i]))
     u++;
     else
     l++;
 }
if(u>l)
{
    for(int i=0;i<s.size();i++)
    {
        char ch;
        ch=toupper(s[i]);
        cout<<ch;
    }
    
}
else
 for(int i=0;i<s.size();i++)
    {
        char ch;
        ch=tolower(s[i]);
        cout<<ch;
    }
    return 0;
    
 
}
//check
// WHENEVER UPPER -->isupper(s[i])
// whenver lower--> islower(s[i])

// implement 
// char c=toupper(s[i])
//char p=tolower(s[i])
