#include <bits/stdc++.h>

    using namespace std;

int main()
{
    int count=0,rount=0,i,n;
   string joy;
   cin>>n;
   cin>>joy;
    for(i=0;i<n;i++)
    {
        if(joy[i]=='A')
            count++;
        else
            rount++;
    }
    if(count>rount)
        cout<<"Anton"<<endl;
        else if(rount>count)
            cout<<"Danik"<<endl;
        if(count==rount)
        cout<<"Friendship"<<endl;
}
 




//2nd method

#include<iostream>
using namespace std;
int main()
{
    int n;
    cin>>n;
    string s;
    int res,res1=0;
    cin>>s;
    char a='A';
    char d='D';
 
    for (int i=0;i<s.length();i++)
 {
        if (s[i] == a)
            res++;
             if (s[i] == d)
            res1++;
           
 }
 if(res>res1)
   cout<<"Anton";
   else if(res<res1)
   cout<<"Danik";
   if(res==res1)
   cout<<"Friendship";
    
}
