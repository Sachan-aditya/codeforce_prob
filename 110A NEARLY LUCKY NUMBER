#include<iostream>
using namespace std;
int main(){
   long long n,c=0;
    cin>>n;
    while(n>0){
       long  long dig=n%10;
        if(dig==4 || dig==7){
            c++;
        }
        n=n/10;

    }
    if(c==4 || c==7)
        cout<<"YES"<<endl;
    
else
    cout<<"NO"<<endl;

return 0;

}
